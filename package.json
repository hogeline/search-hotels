{
  "name": "search-hotels",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=8.12.0 <10.11.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^6.1.0",
    "gh-pages": "^2.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-google-map": "^3.1.1",
    "react-google-maps": "^9.4.5",
    "react-google-maps-loader": "^4.2.3",
    "react-scripts": "2.0.4"
  },
  "scripts": {
    "build-css": "postcss src/styles/main.css -o public/stylesheet.css",
    "watch-css": "postcss src/styles/main.css -o public/stylesheet.css -w",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "start": "npm-run-all -p watch-css start-js",
    "test": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "preghpages": "yarn build",
    "ghpages": "gh-pages -d build"
  },
  "homepage": "./",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "cssnano": "^4.1.4",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0",
    "postcss-import": "^12.0.0",
    "postcss-nested": "^4.1.0",
    "postcss-simple-vars": "^5.0.1"
  }
}
